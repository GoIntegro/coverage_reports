{% extends "index.twig" %}

{% block content %}
<h1>Reportes de Coverage</h1>
<div style="width:80%; ">
<table class="table table-bordered table-stripped">
    <thead>
        <tr>
            <th>Proyecto</th>
            <th>Ultima actualizacion</th>
        </tr>
    </thead>
    <tbody>
        {% for report in reports %}
        <tr>
            <td><a href="{{report.url}}">{{ report.name }}</a></td>
            <td>{{report.last_update}}</td>
        </tr>
        {% endfor %}
    </tbody>
</table>
<div>
{% endblock %}
